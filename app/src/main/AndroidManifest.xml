        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ExtratoActivity" />
        <activity
            android:name=".ListaExtratoActivity"
            android:label="@string/title_activity_lista_extrato"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DetalhesExtratoActivity" />
        <activity android:name=".SaqueActivity" />
        <activity android:name=".ResultadoSaqueActivity"></activity>
    </application>

</manifest>